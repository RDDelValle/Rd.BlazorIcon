@inherits BootstrapIconBase
<svg xmlns="http://www.w3.org/2000/svg" 
     class="@CssClass" 
     fill="@Fill" 
     viewBox="@ViewBox"
     aria-hidden="@AriaHidden.ToString().ToLower()"
     focusable="@Focusable">
    @if (!string.IsNullOrEmpty(Title))
    {
        <title>@Title</title>
    }
    @((MarkupString)Icon!)
</svg>